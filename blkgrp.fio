[global]                
runtime=30
size=512m               

[sdb]
filename=/dev/sdb 
ioengine=libaio         
iodepth=32               
rw=randwrite            
bs=40k                   
direct=1 

[sdc]
filename=/dev/sdc
ioengine=libaio         
iodepth=32               
rw=randwrite            
bs=40k                   
direct=1 

